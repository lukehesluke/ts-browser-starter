{
  "name": "@ts-with-prettier-starter-projects/browser-with-react",
  "version": "0.0.0",
  "description": "Fairly minimal browser TypeScript starter project",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "test": "npm run lint && npm run prettier-format && npm run ts-check",
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "build:watch": "npm run clean && webpack --watch",
    "serve": "cd dist && http-server",
    "lint": "eslint src --ext .ts --ext .tsx",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write",
    "start:dev": "webpack serve --live-reload"
  },
  "author": "Luke Winship",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.3.90",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "prettier": "^2.8.8",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": "18.16.0"
  }
}