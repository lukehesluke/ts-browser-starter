{
  "name": "@ts-with-prettier-starter-projects/branch-name",
  "version": "0.0.0",
  "description": "Fairly minimal TypeScript starter project",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "test": "npm run lint && npm run prettier-format && npm run ts-check",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "author": "Luke Winship",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
